{
  "source": {
    "include": [
      "./src/",
      "./index.js",
      "./README.md"
    ],
    "includePattern": "\\.(js|jsx)$",
    "exclude": [
      "node_modules/",
      "test/",
      "docs/",
      "examples/",
      "benchmarks/"
    ]
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md",
    "package": "./package.json",
    "template": "node_modules/docdash"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "collapse": true,
    "typedefs": true,
    "removeQuotes": "none",
    "scripts": [],
    "menu": {
      "GitHub": {
        "href": "https://github.com/yfbsei/J-Bitcoin",
        "target": "_blank",
        "class": "menu-item",
        "id": "github-link"
      },
      "NPM": {
        "href": "https://www.npmjs.com/package/j-bitcoin",
        "target": "_blank",
        "class": "menu-item",
        "id": "npm-link"
      }
    }
  }
}